* {
    box-sizing: border-box;
}
*:before,
*:after {
    box-sizing: border-box;
}
* {
    margin: 0;
    padding: 0;
}
ul, ol {
    list-style: none;
}
a {
    color: inherit;
    text-decoration: none;
}
img {
    max-width: 100%;
    max-height: 100%;
}
@media (max-width:450px) {
    body {
        display: flex;
        flex-direction: column;
        background: #dddddd;
        color: #1d1c1c;
    }
    .globalHeader {
        width: 100%;
        height: 40vw;
        display: flex;
        flex-direction: column;
    }
    .webTop {
        display: flex;
        justify-content: space-between;
        align-items: center;
    }
    .webTop .webContents {
        border-style: none;
        height: 10vw;
        width: 10vw;
        margin: 1vw;
        cursor: pointer;
    }
    .webTop .webFeedback {
        border-style: none;
        height: 10vw;
        width: 10vw;
        margin: 1vw;
        cursor: pointer;
    }
    /*移动端可以没有cursor: pointer;*/
    .webTop .webIcon {
        font-size: 7vw;
        font-weight: 1000;
    }
    .webTheme {
        display: flex;
        margin-left:70vw;
        margin-right: 3vw;
        justify-content: space-between;

    }

    .webTheme .icon2 {
        border-style: none;
        height: 7vw;
        width: 7vw;
        cursor: pointer;
    }
    /*后续插入图标*/
    .searchEngine {
        display: flex;
        padding-left: 10vw;
    }
    .searchEngine .searchPortal1 {
        margin-left: 5vw;
        height: 6vw;
        font-size: 4vw;
    }
    .searchEngine .searchPortal2 {
        margin-left: 5vw;
        height: 6vw;
        font-size: 4vw;
    }
    .searchEngine .searchPortal3 {
        margin-left: 5vw;
        height: 6vw;
        font-size: 4vw;
    }
    .searchForm {
        width: 90vw;
        border: 2px solid #00B4CC;
        border-radius: 25px;
        margin: 0 auto;
        padding: 1vw 4vw;
        background: #ffffff;
        display: flex;
        justify-content: space-between;
        align-items: center;
    }

    .searchForm .searchInput {
        width: 60vw;
        height: 8vw;
        font-size: 4vw;
        border-style: none;
        outline: none;
        background: #ffffff;
    }
    .searchForm .clear {
        align-items: center;
        cursor: pointer;
        font-size: 8vw;
        background: #ffffff;
        border-style: none;
        margin-right: 1vw;
    }
    .searchForm .searchButton {
        align-items: center;
        cursor: pointer;
        font-size: 8vw;
        background: #ffffff;
        border-style: none;
    }
    .globalMain {
        /*max-width: 900px;*/
        width: 100%;
    }
    .siteList {
        margin: 50px auto;
        width: 84vw;
        display: flex;
        flex-wrap: wrap;
        align-content: flex-start;
    }

    .siteList .site {
        width: 28vw;
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        align-items: center;
        cursor: pointer;
        position: relative;
    }
    .siteList .site .logo {
        width: 20vw;
        height: 20vw;
        border-radius: 50%;
        background: #ffffff;
        display: flex;
        justify-content: center;
        align-items: center;
        font-size: 10vw;
    }
    .siteList .site .link {
        font-size: 4vw;
        margin: 1vw 0 4vw 0;
        max-width: 20vw;
        max-height: 5vw;
        overflow: hidden;
    }
    .siteList .site .close {
        position: absolute;
        right: 10px;
        top: 5px;
        display: none;
        cursor: default;
    }
    .siteList .site:hover .close {
        display: block;
    }
    .siteList .addButton {
        width: 28vw;
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        align-items: center;
    }
    .siteList .addButton .iconWrapper {
        width: 20vw;
        height: 20vw;
        border-radius: 50%;
        background: #ffffff;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .siteList .addButton .text {
        font-size: 4vw;
        margin: 1vw 0 4vw 0;
    }
    .siteList .addButton .iconWrapper .icon {
        width: 80%;
        height: 80%;
        border-style: none;
    }
    .globalFooter {
        border: 2px solid green;
        background: #355095;
        width: 100%;
        height: 50px;
    }
}

@media (min-width: 450px) {
    body {
        display: flex;
        flex-direction: column;
        align-items: center;
        background: #dddddd;
        color: #1d1c1c;
    }
    .globalHeader {
        width: 100%;
        height: 250px;
        display: flex;
        flex-direction: column;
    }
    .webTop {
        padding-top: 20px;
        display: flex;
        justify-content: space-between;
        align-items: center;
    }
    .webTop .webContents {
        /*目录暂无*/
        height: 0;
        width: 0;
    }
    .webTop .webFeedback {
        margin-right: 20px;
        border-style: none;
        height: 30px;
        width: 30px;
        cursor: pointer;
    }
    .webTop .webIcon {
        font-size: 0;
    }
    .webTheme {
        display: flex;
        justify-content: flex-end;
        margin:15px 8px 80px 0;
    }

    .webTheme .icon2 {
        border-style: none;
        height: 20px;
        width: 20px;
        margin-right: 10px;
        cursor: pointer;
    }
    /*后续插入图标*/
    .searchEngine {
       max-width: 560px;
        width: 90vw;
        margin: 0 auto 10px auto;
        display: flex;
        justify-content: flex-start;
        padding-left:40px ;
    }
    .searchEngine .searchPortal1 {
        font-size: 16px;
        padding-right: 15px;
        cursor: pointer;
    }
    .searchEngine .searchPortal2 {
        cursor: pointer;
        font-size: 16px;
        padding-right: 15px;
    }
    .searchEngine .searchPortal3 {
        font-size: 16px;
        padding-right: 15px;
        cursor: pointer;
    }
    .searchForm {
        width: 90vw;
        max-width: 560px;
        height: 46px;
        border: 2px solid #00B4CC;
        border-radius: 23px;
        margin: 0 auto;
        background: #ffffff;
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    .searchForm .searchInput {
        width: 60vw;
        max-width: 400px;
        height: 40px;
        margin-left: 22px;
        font-size: 18px;
        border-style: none;
        outline: none;
        background: #ffffff;
    }
    .searchForm .clear {
        cursor: pointer;
        font-size: 32px;
        background: #ffffff;
        border-style: none;
    }
    .searchForm .searchButton {
        margin-right: 17px;
        cursor: pointer;
        font-size: 32px;
        background: #ffffff;
        border-style: none;
    }
    .globalMain {
        width: 100%;
    }
    .siteList {
        max-width:560px;
        width: 90vw;
        margin: 40px auto;
        display: flex;
        flex-wrap: wrap;
    }
    .siteList .site {
        width: 112px;
        height:112px;
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        align-items: center;
        cursor: pointer;
        position: relative;
    }
    .siteList .site .logo {
        width: 60px;
        height: 60px;
        border-radius: 50%;
        background: #ffffff;
        display: flex;
        justify-content: center;
        align-items: center;
        font-size: 34px;
    }
    .siteList .site .link {
        font-size: 14px;
        max-width: 90px;
        max-height: 20px;
        overflow: hidden;
    }
    .siteList .site .close {
        position: absolute;
        border-radius: 50%;
        right: 4px;
        top: 4px;
        display: none;
        cursor: default;
    }
    .siteList .site:hover .close {
        display: block;
    }
    .siteList .addButton {
        width: 112px;
        height:112px;
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        align-items: center;
        cursor: pointer;
    }
    .siteList .addButton .iconWrapper {
        width: 60px;
        height: 60px;
        border-radius: 50%;
        background: #ffffff;
        display: flex;
        justify-content: center;
        align-items: center;

    }
    .siteList .addButton .text {
        font-size: 14px;
    }
    .siteList .addButton .iconWrapper .icon {
        width: 70%;
        height: 70%;
        border-style: none;
    }
}





