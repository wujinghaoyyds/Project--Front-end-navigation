* {box-sizing: border-box;
}
*:before,
*:after {
    box-sizing: border-box;
}
* {
    margin: 0;
    padding: 0;
}
ul, ol {
    list-style: none;
}
a {
    color: inherit;
    text-decoration: none;
}
img {
    max-width: 100%;
    max-height: 100%;
}
/* style */
body {
    background: #F2F2F2;
}

.globalHeader {
    outline: 1px solid black;
    width:100%;
    height: 160px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.webIcon {
    border: 1px solid red;
    width: 100%;
    height: 50px;
}
.engineBar {
    border: 1px solid red;
    width: 100%;
    height: 30px;
}
.searchForm {
    border: 1px solid blue;
    display: flex;
    width: 100%;
    height: 50px;
    justify-content: space-between;
    /*align-items: center;*/
}
.searchForm .searchInput {
    width: 100%;
    height: 40px;
    border: 3px solid #39b4c4;
    border-radius: 20px 0 0 20px;
    padding: 5px;
    outline:none;
    color: #9DBFAF;
    margin:20px;
}
.searchForm .searchButton {
    width: 40px;
    height: 40px;
    border: 3px solid #39b4c4;
    border-radius: 0 20px 20px 0;
    padding: 5px;
    text-align:center;
    color:#fff;
    cursor: pointer;
    font-size: 20px;
}
