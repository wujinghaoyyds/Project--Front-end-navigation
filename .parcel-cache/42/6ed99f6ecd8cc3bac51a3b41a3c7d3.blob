@media (min-width: 500px) {
    body {
        display: flex;
        flex-direction: column;
        align-items: center;
        background: #e7dddd;
        color: #1d1c1c;
    }
    .globalHeader {
        background: #84a863;
        width: 90%;
        height: 200px;
        display: flex;
        flex-direction: column;
        align-content: space-between;
    }
    .webTop {
        border: 2px solid black;
        display: flex;
        justify-content: space-between;
        align-items: center;
        background: #ffffff;
    }

    .webTop .icon1 {
        border: 2px solid black;
        /*border-style: none;*/
        height: 30px;
        width: 22px;
        cursor: pointer;
    }
    /*移动端可以没有cursor: pointer;*/
    .webTop .webIcon {
        border: 2px solid black;
        font-size: 22px;
        font-weight: 0;
    }
    .webTheme {
        display: flex;
        justify-content: flex-end;
    }

    .webTheme .icon2 {
        background: #4e8e4c;
        /*border-style: none;*/
        height:30px;
        width:30px;
        margin-right:15px;
        cursor: pointer;
    }
    /*后续插入图标*/
    .searchEngine {
        width:500px;
        margin: 0 auto 10px auto;
        display: flex;
        justify-content: flex-start;
        padding-right:300px;
    }
    .searchEngine .searchPortal1 {
        border: 2px solid black;
        font-size: 16px;
        padding-right:15px;
    }
    .searchEngine .searchPortal2 {
        border: 2px solid black;
        font-size: 16px;
        padding-right:15px;
    }
    .searchEngine .searchPortal3 {
        border: 2px solid black;
        font-size: 16px;
        padding-right:15px;
    }
    .searchForm {
        width: 560px;
        height:46px;
        border: 2px solid #00B4CC;
        border-radius: 23px;
        margin: 0 auto;
        background: #ffffff;
        display: flex;
        align-items: center;
        justify-content: space-between;
    }
    .searchForm .searchInput {
        width: 420px;
        height: 40px;
        margin-left:22px;
        font-size: 18px;
        border-style: none;
        outline: none;
        background: #ffffff;
    }
    .searchForm .clear {
        cursor: pointer;
        font-size: 32px;
        background: #ffffff;
        border-style: none;
    }
    .searchForm .searchButton {
        margin-right:20px;
        cursor: pointer;
        font-size: 32px;
        background: #ffffff;
        border-style: none;
    }
    .globalMain {
        border: 2px solid black;
        /*max-width: 900px;*/
        width: 100%;
    }
    .siteList {
        border: 2px solid black;
        margin: 5px auto;
        width: 30vw;
        display: flex;
        flex-wrap: wrap;
        align-content: flex-start;
    }

    .siteList .site {
        border: 2px solid black;
        width: 8vw;
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        align-items: center;
        cursor: pointer;
        position: relative;
    }
    .siteList .site .logo {
        border: 2px solid black;
        width: 8vw;
        height: 6vw;
        border-radius: 50%;
        background: #ffffff;
        display: flex;
        justify-content: center;
        align-items: center;
        font-size: 4vw;
    }
    .siteList .site .link {
        border: 2px solid black;
        font-size: 1vw;
        margin: 1vw 0 2vw 0;
        max-width: 5vw;
        max-height: 5vw;
        overflow: hidden;
    }
    .siteList .site .close {
        border: 2px solid black;
        position: absolute;
        right: 4px;
        top: 3px;
        display: none;
        cursor: default;
    }
    .siteList .site:hover .close {
        display: block;
    }
    .siteList .addButton {
        border: 2px solid black;
        width: 8vw;
        display: flex;
        flex-direction: column;
        justify-content: space-evenly;
        align-items: center;
    }
    .siteList .addButton .iconWrapper {
        border: 2px solid black;
        width: 6vw;
        height: 4vw;
        border-radius: 50%;
        background: #ffffff;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .siteList .addButton .text {
        border: 2px solid black;
        font-size: 2vw;
        margin: 1vw 0 4vw 0;
    }
    .siteList .addButton .iconWrapper .icon {
        border: 2px solid black;
        width: 80%;
        height: 80%;
        /*border-style: none;*/
    }
}





